---
import { supabase } from "@/functions/supabase";
import Layout from "@/layouts/Layout.astro";
import Testimonials from "@/components/sections/Testimonials.astro";
import HeaderPage from "@/components/sections/HeaderPage.astro";
import Team from "@/components/sections/Team.astro";

const { data, error } = await supabase.from('equipo').select().eq('tipo',2);
if(error) return console.log(error);
const equipos = data
---

<Layout title="Consultores / Facilitadores | Stratega Consultoría & Capacitación">
  <HeaderPage title="Consultores / Facilitadores" />
  <Team team={equipos} page="consultores" />
  <Testimonials />
</Layout>