---
import Layout from "@/layouts/Layout.astro";
import Testimonials from "@/components/sections/Testimonials.astro";
import HeaderPage from "@/components/sections/HeaderPage.astro";
import Team from "@/components/sections/Team.astro";
import { getCollection } from 'astro:content';
const equipoPost = await getCollection('equipo');
---

<Layout title="Equipo Stratega | Stratega Consultoría & Capacitación">
  <HeaderPage title="Equipo Stratega" />
  <Team team={equipoPost} page="equipo" />
  <Testimonials />
</Layout>