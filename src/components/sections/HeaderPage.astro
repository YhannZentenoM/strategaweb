---
import Title from '@/components/Title.astro';
import Major from '@/icons/Major.astro';
const { title } = Astro.props;
---
<section class="h-[55vh] flex items-end justify-center pb-16 bg-secondary">
    <div class="flex flex-col items-center gap-y-3">
        <Title title={`${title}`} subtitle="" align="text-center" />
        <Major class="w-9" />
    </div>
</section>